{
  "name": "renepardon/laravel-google-geocoding",
  "description": "Easy to use API to retrieve geocoding results from Google in your Laravel application levering cache.",
  "keywords": [
    "fhusquinet",
    "laravel-google-geocoding",
    "geocoding",
    "laravel"
  ],
  "homepage": "https://github.com/renepardon/laravel-google-geocoding",
  "license": "MIT",
  "authors": [
    {
      "name": "Florian Husquinet",
      "email": "florian.husquinet@deegital.be",
      "homepage": "https://www.deegital.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0|^8.1",
    "illuminate/config": "~5.5.0|~5.6.0|~5.7.0|^6.0|^7.0|^8.0",
    "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|^6.0|^7.0|^8.0",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|^6.0|^7.0|^8.0",
    "guzzlehttp/guzzle": "^7.2"
  },
  "require-dev": {
    "orchestra/testbench": "^v6.24",
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.5.0"
  },
  "autoload": {
    "psr-4": {
      "FHusquinet\\GoogleGeocoding\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FHusquinet\\GoogleGeocoding\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "FHusquinet\\GoogleGeocoding\\GoogleGeocodingServiceProvider"
      ]
    }
  }
}
